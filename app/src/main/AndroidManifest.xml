<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hotelreservation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity
            android:name=".GetMenuActivity"
            android:theme="@style/DashBoardTheme" />
        <activity
            android:name=".DinnerActivity"
            android:theme="@style/DashBoardTheme" />
        <activity
            android:name=".TeaTimeActivity"
            android:theme="@style/DashBoardTheme" />
        <activity
            android:name=".LunchActivity"
            android:theme="@style/DashBoardTheme" />
        <activity
            android:name=".MaindashboardActivity"
            android:theme="@style/DashBoardTheme" />
        <activity
            android:name=".SpaActivity"
            android:theme="@style/DashBoardTheme" />
        <activity
            android:name=".ActivitiesActivity"
            android:theme="@style/DashBoardTheme" />
        <activity
            android:name=".RoomserviceActivity"
            android:theme="@style/DashBoardTheme" />
        <activity
            android:name=".RestaurantActivity"
            android:theme="@style/DashBoardTheme" />
        <activity
            android:name=".DashboardActivity"
            android:theme="@style/DashBoardTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GetserviceActivity"
            android:theme="@style/LoginAppTheme" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme" />
    </application>

</manifest>